{
    "manifest_version": 2,
    "name": "RickRollify",
    "description": "1/100 chance to rick roll you",
    "version": "1.0",
    "icons": {
        "16": "/images/icon.png",
        "48": "/images/icon_48.png",
        "128": "/images/icon_128.png"
    },
    "page_action": {
        "default_icon": {
            "16": "/images/icon.png",
            "48": "/images/icon_48.png",
            "128": "/images/icon_128.png"
        }
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["main.js"]
    }]
}